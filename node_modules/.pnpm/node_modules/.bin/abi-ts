#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules/@latticexyz/abi-ts/dist/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules/@latticexyz/abi-ts/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules/@latticexyz/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules/@latticexyz/abi-ts/dist/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules/@latticexyz/abi-ts/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules/@latticexyz/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/@latticexyz+abi-ts@2.0.0-next.14/node_modules:/Users/jaesungpark/code/apollyon2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@latticexyz/abi-ts/dist/abi-ts.js" "$@"
else
  exec node  "$basedir/../@latticexyz/abi-ts/dist/abi-ts.js" "$@"
fi
